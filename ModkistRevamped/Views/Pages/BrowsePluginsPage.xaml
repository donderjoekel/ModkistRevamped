<Page
    x:Class="TNRD.Modkist.Views.Pages.BrowsePluginsPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:list="clr-namespace:TNRD.Modkist.Views.Controls.List"
    xmlns:local="clr-namespace:TNRD.Modkist.Views.Pages"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml"
    Title="BrowseBlueprintsPage"
    d:DataContext="{d:DesignInstance local:BrowsePluginsPage,
                                     IsDesignTimeCreatable=False}"
    ui:Design.Background="{DynamicResource ApplicationBackgroundBrush}"
    ui:Design.Foreground="{DynamicResource TextFillColorPrimaryBrush}"
    Foreground="{DynamicResource TextFillColorPrimaryBrush}"
    ScrollViewer.CanContentScroll="False"
    mc:Ignorable="d">
    <Grid>
        <list:ModList ListMode="Card" ModType="Plugin" />
    </Grid>
</Page>
